<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="XML2JSON.Azure" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2014-01.2.3">
  <WebRole name="XML2JSON.Web.OWIN" vmsize="ExtraSmall">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="80" />
    </Endpoints>
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
     <LocalResources>
      <LocalStorage name="fileCache" cleanOnRoleRecycle="false" />
    </LocalResources> 
  </WebRole>
</ServiceDefinition>